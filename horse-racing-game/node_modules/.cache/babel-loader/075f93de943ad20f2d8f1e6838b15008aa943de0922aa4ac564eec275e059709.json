{"ast":null,"code":"import { mapState, mapActions, mapMutations } from 'vuex';\nexport default {\n  computed: {\n    ...mapState(['horses', 'raceInterval']),\n    raceRunning() {\n      return !!this.raceInterval; // Yarış animasyonu çalışıyor mu?\n    }\n  },\n  methods: {\n    ...mapActions(['generateHorses', 'startRace', 'updateHorsePosition']),\n    ...mapMutations(['SET_RACE_INTERVAL', 'CLEAR_RACE_INTERVAL']),\n    async handleGenerateScheduleClick() {\n      try {\n        await this.generateHorses();\n        console.log('Horses generated and race schedule created successfully!');\n      } catch (error) {\n        console.error('Error generating horses and creating race schedule:', error);\n        alert('Error generating horses and creating race schedule. Please try again later.');\n      }\n    },\n    async handleStartRaceClick() {\n      if (this.raceRunning) {\n        this.pauseRace();\n      } else {\n        this.startRace();\n      }\n    },\n    async pauseRace() {\n      this.CLEAR_RACE_INTERVAL();\n      console.log('Race paused.');\n    },\n    startRace() {\n      if (this.horses.length === 0) {\n        alert('Please generate horses first.');\n        return;\n      }\n      const interval = setInterval(() => {\n        this.horses.forEach(horse => {\n          const speed = this.calculateSpeed(horse.condition);\n          let newPosition = horse.position + speed;\n\n          // Ensure horses cannot move beyond 605 pixels\n          if (newPosition > 605) {\n            newPosition = 605;\n          }\n          this.updateHorsePosition({\n            horseId: horse.id,\n            position: newPosition\n          });\n          console.log(`Horse ${horse.id} has moved ${newPosition.toFixed(2)} pixels.`);\n        });\n        const allHorsesFinished = this.horses.every(horse => horse.position >= 605);\n        if (allHorsesFinished) {\n          this.CLEAR_RACE_INTERVAL();\n          console.log('Race finished.');\n        }\n      }, 100);\n      this.SET_RACE_INTERVAL(interval);\n      console.log('Race started.');\n    },\n    calculateSpeed(condition) {\n      return Math.round(condition / 2); // Koşul değeri / 10\n    }\n  },\n  async mounted() {\n    try {\n      await this.generateHorses();\n    } catch (error) {\n      console.error('Error initializing horses and race schedule:', error);\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","mapMutations","computed","raceRunning","raceInterval","methods","handleGenerateScheduleClick","generateHorses","console","log","error","alert","handleStartRaceClick","pauseRace","startRace","CLEAR_RACE_INTERVAL","horses","length","interval","setInterval","forEach","horse","speed","calculateSpeed","condition","newPosition","position","updateHorsePosition","horseId","id","toFixed","allHorsesFinished","every","SET_RACE_INTERVAL","Math","round","mounted"],"sources":["/Users/aleynakoluk/Desktop/horse-racing-game/src/components/ControlButtons.vue"],"sourcesContent":["<template>\n  <div class=\"header-container\">\n    <header class=\"header\">\n      <div class=\"left\">\n        <h1>Horse Racing</h1>\n      </div>\n      <div class=\"right\">\n        <button @click=\"handleGenerateScheduleClick\" class=\"btn\">GENERATE PROGRAM</button>\n        <button @click=\"handleStartRaceClick\" class=\"btn\">START/PAUSE</button>\n      </div>\n    </header>\n    <!-- Diğer içerikler buraya gelecek -->\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from 'vuex';\n\nexport default {\n  computed: {\n    ...mapState(['horses', 'raceInterval']),\n    raceRunning() {\n      return !!this.raceInterval; // Yarış animasyonu çalışıyor mu?\n    },\n  },\n  methods: {\n    ...mapActions(['generateHorses', 'startRace', 'updateHorsePosition']),\n    ...mapMutations(['SET_RACE_INTERVAL', 'CLEAR_RACE_INTERVAL']),\n    async handleGenerateScheduleClick() {\n      try {\n        await this.generateHorses();\n        console.log('Horses generated and race schedule created successfully!');\n      } catch (error) {\n        console.error('Error generating horses and creating race schedule:', error);\n        alert('Error generating horses and creating race schedule. Please try again later.');\n      }\n    },\n    async handleStartRaceClick() {\n      if (this.raceRunning) {\n        this.pauseRace();\n      } else {\n        this.startRace();\n      }\n    },\n    async pauseRace() {\n      this.CLEAR_RACE_INTERVAL();\n      console.log('Race paused.');\n    },\n    startRace() {\n      if (this.horses.length === 0) {\n        alert('Please generate horses first.');\n        return;\n      }\n\n      const interval = setInterval(() => {\n        this.horses.forEach(horse => {\n          const speed = this.calculateSpeed(horse.condition);\n          let newPosition = horse.position + speed;\n\n          // Ensure horses cannot move beyond 605 pixels\n          if (newPosition > 605) {\n            newPosition = 605;\n          }\n\n          this.updateHorsePosition({ horseId: horse.id, position: newPosition });\n          console.log(`Horse ${horse.id} has moved ${newPosition.toFixed(2)} pixels.`);\n        });\n\n        const allHorsesFinished = this.horses.every(horse => horse.position >= 605);\n        if (allHorsesFinished) {\n          this.CLEAR_RACE_INTERVAL();\n          console.log('Race finished.');\n        }\n      }, 100); \n\n      this.SET_RACE_INTERVAL(interval);\n      console.log('Race started.');\n    },\n    calculateSpeed(condition) {\n      return Math.round(condition / 2); // Koşul değeri / 10\n    },\n  },\n  async mounted() {\n    try {\n      await this.generateHorses();\n    } catch (error) {\n      console.error('Error initializing horses and race schedule:', error);\n    }\n  },\n};\n</script>\n\n<style scoped>\n.header-container {\n  background-color: #f08080; /* Pembe arka plan */\n  width: 100%;\n}\n\n.header {\n  position: sticky; /* Yapışkan pozisyon */\n  top: 0; /* Ekranın en üstüne yapıştır */\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  height: 93px; /* Header yüksekliği */\n  padding: 0 20px; /* Sol ve sağ padding */\n}\n\n.left h1 {\n  margin: 0;\n  color: #333;\n}\n\n.right {\n  display: flex;\n  gap: 10px;\n}\n\n.btn {\n  padding: 10px 50px;\n  cursor: pointer;\n  background-color: #d8d8d8fe;\n  color: black;\n  border: 1px solid #333;\n  border-radius: 4px;\n}\n</style>\n"],"mappings":"AAgBA,SAASA,QAAQ,EAAEC,UAAU,EAAEC,YAAW,QAAS,MAAM;AAEzD,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGH,QAAQ,CAAC,CAAC,QAAQ,EAAE,cAAc,CAAC,CAAC;IACvCI,WAAWA,CAAA,EAAG;MACZ,OAAO,CAAC,CAAC,IAAI,CAACC,YAAY,EAAE;IAC9B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,gBAAgB,EAAE,WAAW,EAAE,qBAAqB,CAAC,CAAC;IACrE,GAAGC,YAAY,CAAC,CAAC,mBAAmB,EAAE,qBAAqB,CAAC,CAAC;IAC7D,MAAMK,2BAA2BA,CAAA,EAAG;MAClC,IAAI;QACF,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;QAC3BC,OAAO,CAACC,GAAG,CAAC,0DAA0D,CAAC;MACzE,EAAE,OAAOC,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,qDAAqD,EAAEA,KAAK,CAAC;QAC3EC,KAAK,CAAC,6EAA6E,CAAC;MACtF;IACF,CAAC;IACD,MAAMC,oBAAoBA,CAAA,EAAG;MAC3B,IAAI,IAAI,CAACT,WAAW,EAAE;QACpB,IAAI,CAACU,SAAS,CAAC,CAAC;MAClB,OAAO;QACL,IAAI,CAACC,SAAS,CAAC,CAAC;MAClB;IACF,CAAC;IACD,MAAMD,SAASA,CAAA,EAAG;MAChB,IAAI,CAACE,mBAAmB,CAAC,CAAC;MAC1BP,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;IAC7B,CAAC;IACDK,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAACE,MAAM,CAACC,MAAK,KAAM,CAAC,EAAE;QAC5BN,KAAK,CAAC,+BAA+B,CAAC;QACtC;MACF;MAEA,MAAMO,QAAO,GAAIC,WAAW,CAAC,MAAM;QACjC,IAAI,CAACH,MAAM,CAACI,OAAO,CAACC,KAAI,IAAK;UAC3B,MAAMC,KAAI,GAAI,IAAI,CAACC,cAAc,CAACF,KAAK,CAACG,SAAS,CAAC;UAClD,IAAIC,WAAU,GAAIJ,KAAK,CAACK,QAAO,GAAIJ,KAAK;;UAExC;UACA,IAAIG,WAAU,GAAI,GAAG,EAAE;YACrBA,WAAU,GAAI,GAAG;UACnB;UAEA,IAAI,CAACE,mBAAmB,CAAC;YAAEC,OAAO,EAAEP,KAAK,CAACQ,EAAE;YAAEH,QAAQ,EAAED;UAAY,CAAC,CAAC;UACtEjB,OAAO,CAACC,GAAG,CAAC,SAASY,KAAK,CAACQ,EAAE,cAAcJ,WAAW,CAACK,OAAO,CAAC,CAAC,CAAC,UAAU,CAAC;QAC9E,CAAC,CAAC;QAEF,MAAMC,iBAAgB,GAAI,IAAI,CAACf,MAAM,CAACgB,KAAK,CAACX,KAAI,IAAKA,KAAK,CAACK,QAAO,IAAK,GAAG,CAAC;