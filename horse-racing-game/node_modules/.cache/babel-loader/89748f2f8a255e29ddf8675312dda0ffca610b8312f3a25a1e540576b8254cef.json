       <tr>\n            <th>Horse ID</th>\n            <th>Condition</th>\n            <th>Color</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"horse in horses\" :key=\"horse.id\">\n            <td>{{ horse.id }}</td>\n            <td>{{ horse.condition }}</td>\n            <td :style=\"{ color: horse.color }\">{{ horse.color }}</td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n    <div class=\"race-track-container\">\n      <div class=\"race-track\">\n        <img src=\"@/assets/race-track.png\" alt=\"Race Track\" class=\"race-track-img\" />\n        <div v-for=\"horse in horses\" :key=\"horse.id\" class=\"horse\" :style=\"{ left: horse.position + '%' }\">\n          <img :src=\"horse.image\" :alt=\"'Horse ' + horse.id\" class=\"horse-img\" />\n        </div>\n      </div>\n      <div class=\"winner\" v-if=\"winner !== null\">\n        Winner: Horse {{ winner }}\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HorseRace',\n  computed: {\n    horses() {\n      return this.$store.state.horses;\n    },\n    winner() {\n      return this.$store.state.raceResults.length > 0 ? this.$store.state.raceResults[0].horseId : null;\n    }\n  },\n  mounted() {\n    this.$store.dispatch('generateHorses');\n    setTimeout(() => {\n      this.$store.dispatch('startRace');\n    }, 1000);\n  }\n};\n</script>\n\n<style scoped>\n.race-container {\n  display: flex;\n  justify-content: flex-start;\n}\n\n.horse-list {\n  background-color: #f8f9fa;\n  border: 1px solid #ddd;\n  padding: 20px;\n  margin-right: 20px;\n  border-radius: 4px;\n}\n\n.horse-list h2 {\n  color: #333;\n  margin-bottom: 10px;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nth, td {\n  text-align: left;\n  padding: 8px;\n  border: 1px solid #ddd;\n}\n\nth {\n  background-color: #f2f2f2;\n}\n\n.race-track-container {\n  flex-grow: 1;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.