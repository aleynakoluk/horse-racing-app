ore.state.raceResults.length > 0 ? this.$store.state.raceResults[0].horseId : null;\n      }\n    },\n    mounted() {\n      this.$store.dispatch('generateHorses');\n      setTimeout(() => {\n        this.$store.dispatch('startRace');\n      }, 1000);\n    }\n  };\n  </script>\n  \n  <style scoped>\n  .race-track {\n    background-image: url('@/assets/race-track.png');\n    background-size: cover;\n    height: 300px;\n    position: relative;\n    margin-bottom: 20px;\n  }\n  \n  .horse {\n    position: absolute;\n    bottom: 10px;\n  }\n  \n  .horse-img {\n    width: 50px; /* Ayarlamak gerekiyorsa boyutu değiştirin */\n  }\n  \n  .winner {\n    position: absolute;\n    bottom: 20px;\n    left: 50%;\n    transform: translateX(-50%);\n    font-size: 18px;\n    font-weight: bold;\n    color: green;\n  }\n  </style>\n  "],"mappings":";;;EA6ESA,KAAK,EAAC;AAAY;mBA7E3B;;EAAAC,GAAA;EAiFWD,KAAK,EAAC;;;uBAJbE,mBAAA,CAOM,OAPNC,UAOM,I,kBANJD,mBAAA,CAEME,SAAA,QAhFZC,WAAA,CA8E2BC,QAAA,CAAAC,MAAM,EAAfC,KAAK;yBAAjBN,mBAAA,CAEM;MAFwBD,GAAG,EAAEO,KAAK,CAACC,EAAE;MAAET,KAAK,EAAC,OAAO;MAAEU,KAAK,EA9EvEC,eAAA;QAAAC,IAAA,EA8EiFJ,KAAK,CAACK,QAAQ;MAAA;QACvFC,mBAAA,CAAwD;MAAlDC,GAAG,EAAEP,KAAK,CAACQ,KAAK;MAAEC,GAAG,EAAC,OAAO;MAACjB,KAAK,EAAC;4BA/ElDkB,UAAA,E;kCAiFgCZ,QAAA,CAAAa,MAAM,a,cAAhCjB,mBAAA,CAEM,OAFNkB,UAEM,EAFqC,iBAC3B,GAAAC,gBAAA,CAAGf,QAAA,CAAAa,MAAM,oBAlF/BG,mBAAA,e","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}