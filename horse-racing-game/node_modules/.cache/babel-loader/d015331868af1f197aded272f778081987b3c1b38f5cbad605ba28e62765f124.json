{"ast":null,"code":"import { mapState, mapActions, mapMutations } from 'vuex';\nexport default {\n  computed: {\n    ...mapState(['horses', 'raceInterval']),\n    // Vuex durumunu bileşene aktarma\n    raceRunning() {\n      return !!this.raceInterval; // Yarış animasyonu çalışıyor mu?\n    }\n  },\n  methods: {\n    ...mapActions(['generateHorses', 'startRace', 'updateHorsePosition']),\n    // Vuex eylemlerini bileşene aktarma\n    ...mapMutations(['SET_RACE_INTERVAL', 'CLEAR_RACE_INTERVAL']),\n    // Vuex mutasyonlarını bileşene aktarma\n    async handleGenerateScheduleClick() {\n      // Program oluşturma butonuna tıklandığında tetiklenir\n      try {\n        await this.generateHorses(); // Atları oluşturma ve programı başlatma\n        console.log('Horses generated and race schedule created successfully!');\n      } catch (error) {\n        console.error('Error generating horses and creating race schedule:', error);\n        alert('Error generating horses and creating race schedule. Please try again later.');\n      }\n    },\n    async handleStartRaceClick() {\n      // Yarış başlat/duraklat butonuna tıklandığında tetiklenir\n      if (this.raceRunning) {\n        this.pauseRace(); // Yarış çalışıyorsa duraklatma\n      } else {\n        this.startRace(); // Yarış duruyorsa başlatma\n      }\n    },\n    async pauseRace() {\n      // Yarışı duraklatma\n      this.CLEAR_RACE_INTERVAL(); // Yarış intervalini temizleme\n      console.log('Race paused.');\n    },\n    startRace() {\n      // Yarışı başlatma\n      if (this.horses.length === 0) {\n        alert('Please generate horses first.'); // Atlar yoksa uyarı versin\n        return;\n      }\n      const interval = setInterval(() => {\n        this.horses.forEach(horse => {\n          const speed = this.calculateSpeed(horse.condition); // Atın hızını hesapla\n          let newPosition = horse.position + speed; // Yeni pozisyonu hesapla\n\n          // Atların 605 pikselden fazla hareket etmemesini sağla\n          if (newPosition > 605) {\n            newPosition = 605;\n          }\n          this.updateHorsePosition({\n            horseId: horse.id,\n            position: newPosition\n          }); // Atın pozisyonunu güncelle\n          console.log(`Horse ${horse.id} has moved ${newPosition.toFixed(2)} pixels.`);\n        });\n\n        // Tüm atlar bitiş çizgisine ulaştıysa yarışı bitir\n        const allHorsesFinished = this.horses.every(horse => horse.position >= 605);\n        if (allHorsesFinished) {\n          this.CLEAR_RACE_INTERVAL(); // Yarış intervalini temizle\n          console.log('Race finished.');\n        }\n      }, 100); // 100 ms aralıklarla pozisyonları güncelle\n\n      this.SET_RACE_INTERVAL(interval); // Yarış intervalini Vuex'te kaydet\n      console.log('Race started.');\n    },\n    calculateSpeed(condition) {\n      // Atın hızını hesapla\n      return Math.round(condition / 2);\n    }\n  },\n  async mounted() {\n    // Bileşen yüklendiğinde atları oluştur\n    try {\n      await this.generateHorses();\n    } catch (error) {\n      console.error('Error initializing horses and race schedule:', error);\n    }\n  }\n};","map":{"version":3,"names":["mapState","mapActions","mapMutations","computed","raceRunning","raceInterval","methods","handleGenerateScheduleClick","generateHorses","console","log","error","alert","handleStartRaceClick","pauseRace","startRace","CLEAR_RACE_INTERVAL","horses","length","interval","setInterval","forEach","horse","speed","calculateSpeed","condition","newPosition","position","updateHorsePosition","horseId","id","toFixed","allHorsesFinished","every","SET_RACE_INTERVAL","Math","round","mounted"],"sources":["/Users/aleynakoluk/Desktop/horse-racing-game/src/components/ControlButtons.vue"],"sourcesContent":["<template>\n  <!-- Header konteyneri -->\n  <div class=\"header-container\">\n    <!-- Header bölümü -->\n    <header class=\"header\">\n      <!-- Sol bölüm -->\n      <div class=\"left\">\n        <h1>Horse Racing</h1> <!-- Başlık -->\n      </div>\n      <!-- Sağ bölüm -->\n      <div class=\"right\">\n        <!-- Program oluşturma butonu -->\n        <button @click=\"handleGenerateScheduleClick\" class=\"btn\">GENERATE PROGRAM</button>\n        <!-- Yarış başlat/duraklat butonu -->\n        <button @click=\"handleStartRaceClick\" class=\"btn\">START/PAUSE</button>\n      </div>\n    </header>\n  </div>\n</template>\n\n<script>\nimport { mapState, mapActions, mapMutations } from 'vuex';\n\nexport default {\n  computed: {\n    ...mapState(['horses', 'raceInterval']), // Vuex durumunu bileşene aktarma\n    raceRunning() {\n      return !!this.raceInterval; // Yarış animasyonu çalışıyor mu?\n    },\n  },\n  methods: {\n    ...mapActions(['generateHorses', 'startRace', 'updateHorsePosition']), // Vuex eylemlerini bileşene aktarma\n    ...mapMutations(['SET_RACE_INTERVAL', 'CLEAR_RACE_INTERVAL']), // Vuex mutasyonlarını bileşene aktarma\n    async handleGenerateScheduleClick() {\n      // Program oluşturma butonuna tıklandığında tetiklenir\n      try {\n        await this.generateHorses(); // Atları oluşturma ve programı başlatma\n        console.log('Horses generated and race schedule created successfully!');\n      } catch (error) {\n        console.error('Error generating horses and creating race schedule:', error);\n        alert('Error generating horses and creating race schedule. Please try again later.');\n      }\n    },\n    async handleStartRaceClick() {\n      // Yarış başlat/duraklat butonuna tıklandığında tetiklenir\n      if (this.raceRunning) {\n        this.pauseRace(); // Yarış çalışıyorsa duraklatma\n      } else {\n        this.startRace(); // Yarış duruyorsa başlatma\n      }\n    },\n    async pauseRace() {\n      // Yarışı duraklatma\n      this.CLEAR_RACE_INTERVAL(); // Yarış intervalini temizleme\n      console.log('Race paused.');\n    },\n    startRace() {\n      // Yarışı başlatma\n      if (this.horses.length === 0) {\n        alert('Please generate horses first.'); // Atlar yoksa uyarı versin\n        return;\n      }\n\n      const interval = setInterval(() => {\n        this.horses.forEach(horse => {\n          const speed = this.calculateSpeed(horse.condition); // Atın hızını hesapla\n          let newPosition = horse.position + speed; // Yeni pozisyonu hesapla\n\n          // Atların 605 pikselden fazla hareket etmemesini sağla\n          if (newPosition > 605) {\n            newPosition = 605;\n          }\n\n          this.updateHorsePosition({ horseId: horse.id, position: newPosition }); // Atın pozisyonunu güncelle\n          console.log(`Horse ${horse.id} has moved ${newPosition.toFixed(2)} pixels.`);\n        });\n\n        // Tüm atlar bitiş çizgisine ulaştıysa yarışı bitir\n        const allHorsesFinished = this.horses.every(horse => horse.position >= 605);\n        if (allHorsesFinished) {\n          this.CLEAR_RACE_INTERVAL(); // Yarış intervalini temizle\n          console.log('Race finished.');\n        }\n      }, 100); // 100 ms aralıklarla pozisyonları güncelle\n\n      this.SET_RACE_INTERVAL(interval); // Yarış intervalini Vuex'te kaydet\n      console.log('Race started.');\n    },\n    calculateSpeed(condition) {\n      // Atın hızını hesapla\n      return Math.round(condition / 2); \n    },\n  },\n  async mounted() {\n    // Bileşen yüklendiğinde atları oluştur\n    try {\n      await this.generateHorses();\n    } catch (error) {\n      console.error('Error initializing horses and race schedule:', error);\n    }\n  },\n};\n</script>\n\n<style scoped>\n/* Üst bilgi konteyner stili */\n.header-container {\n  background-color: #f08080;\n  width: 100%;\n}\n\n/* Üst bilgi stili */\n.header {\n  position: sticky;\n  top: 0; /* Ekranın en üstüne yapıştır */\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  height: 93px; /* Header yüksekliği */\n  padding: 0 20px; /* Sol ve sağ padding */\n}\n\n/* Sol bölüm başlık stili */\n.left h1 {\n  margin: 0;\n  color: #333;\n}\n\n/* Sağ bölüm stili */\n.right {\n  display: flex;\n  gap: 10px;\n}\n\n/* Buton stili */\n.btn {\n  padding: 10px 50px;\n  cursor: pointer;\n  background-color: #d8d8d8fe;\n  color: black;\n  border: 1px solid #333;\n  border-radius: 4px;\n}\n</style>\n"],"mappings":"AAqBA,SAASA,QAAQ,EAAEC,UAAU,EAAEC,YAAW,QAAS,MAAM;AAEzD,eAAe;EACbC,QAAQ,EAAE;IACR,GAAGH,QAAQ,CAAC,CAAC,QAAQ,EAAE,cAAc,CAAC,CAAC;IAAE;IACzCI,WAAWA,CAAA,EAAG;MACZ,OAAO,CAAC,CAAC,IAAI,CAACC,YAAY,EAAE;IAC9B;EACF,CAAC;EACDC,OAAO,EAAE;IACP,GAAGL,UAAU,CAAC,CAAC,gBAAgB,EAAE,WAAW,EAAE,qBAAqB,CAAC,CAAC;IAAE;IACvE,GAAGC,YAAY,CAAC,CAAC,mBAAmB,EAAE,qBAAqB,CAAC,CAAC;IAAE;IAC/D,MAAMK,2BAA2BA,CAAA,EAAG;MAClC;MACA,IAAI;QACF,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC,EAAE;QAC7BC,OAAO,CAACC,GAAG,CAAC,0DAA0D,CAAC;MACzE,EAAE,OAAOC,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,qDAAqD,EAAEA,KAAK,CAAC;QAC3EC,KAAK,CAAC,6EAA6E,CAAC;MACtF;IACF,CAAC;IACD,MAAMC,oBAAoBA,CAAA,EAAG;MAC3B;MACA,IAAI,IAAI,CAACT,WAAW,EAAE;QACpB,IAAI,CAACU,SAAS,CAAC,CAAC,EAAE;MACpB,OAAO;QACL,IAAI,CAACC,SAAS,CAAC,CAAC,EAAE;MACpB;IACF,CAAC;IACD,MAAMD,SAASA,CAAA,EAAG;MAChB;MACA,IAAI,CAACE,mBAAmB,CAAC,CAAC,EAAE;MAC5BP,OAAO,CAACC,GAAG,CAAC,cAAc,CAAC;IAC7B,CAAC;IACDK,SAASA,CAAA,EAAG;MACV;MACA,IAAI,IAAI,CAACE,MAAM,CAACC,MAAK,KAAM,CAAC,EAAE;QAC5BN,KAAK,CAAC,+BAA+B,CAAC,EAAE;QACxC;MACF;MAEA,MAAMO,QAAO,GAAIC,WAAW,CAAC,MAAM;QACjC,IAAI,CAACH,MAAM,CAACI,OAAO,CAACC,KAAI,IAAK;UAC3B,MAAMC,KAAI,GAAI,IAAI,CAACC,cAAc,CAACF,KAAK,CAACG,SAAS,CAAC,EAAE;UACpD,IAAIC,WAAU,GAAIJ,KAAK,CAACK,QAAO,GAAIJ,KAAK,EAAE;;UAE1C;UACA,IAAIG,WAAU,GAAI,GAAG,EAAE;YACrBA,WAAU,GAAI,GAAG;UACnB;UAEA,IAAI,CAACE,mBAAmB,CAAC;YAAEC,OAAO,EAAEP,KAAK,CAACQ,EAAE;YAAEH,QAAQ,EAAED;UAAY,CAAC,CAAC,EAAE;UACxEjB,OAAO,CAACC,GAAG,CAAC,SAASY,KAAK,CAACQ,EAAE,cAAcJ,WAAW,CAACK,OAAO,CAAC,CAAC,CAAC,UAAU,CAAC;QAC9E,CAAC,CAAC;;QAEF;QACA,MAAMC,iBAAgB,GAAI,IAAI,CAACf,MAAM,CAACgB,KAAK,CAACX,KAAI,IAAKA,KAAK,CAACK,QAAO,IAAK,GAAG,CAAC;QAC3E,IAAIK,iBAAiB,EAAE;UACrB,IAAI,CAAChB,mBAAmB,CAAC,CAAC,EAAE;UAC5BP,OAAO,CAACC,GAAG,CAAC,gBAAgB,CAAC;QAC/B;MACF,CAAC